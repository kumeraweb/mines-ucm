---
import BaseLayout from '../layouts/BaseLayout.astro'
import Header from '../components/Header.astro'
import PromoBanner from '../components/PromoBanner.astro'
import Hero from '../components/Hero.astro'
import WhyUCM from '../components/WhyUCM.astro'
import Services from '../components/Services.astro'
import PlnaesUCM from '../components/PlanesUCM.astro'
import BeneficiosUCM from '../components/BeneficiosUCM.astro'
import CoberturaUCM from '../components/CoberturaUCM.astro'
import ContrataUCM from '../components/ContrataUCM.astro'
import Footer from '../components/Footer.astro'
import { SITE } from '../data/site'
const meta = {
  title: SITE.title,
  description: SITE.description,
  image: SITE.ogImage
}
---

<BaseLayout {...meta}>
  <Header />
  <main>
    <Hero />
    <PromoBanner />
    <WhyUCM />
    <Services />
    <PlnaesUCM />
    <BeneficiosUCM />
    <CoberturaUCM />
    <ContrataUCM />
    <Footer />
  </main>
</BaseLayout>
